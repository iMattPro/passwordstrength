{
	"name": "vse/passwordstrength",
	"type": "phpbb-extension",
	"description": "An extension for phpBB 3.1 that will show users how strong (or weak) their password is as they type it into the password field when creating or updating their account.",
	"homepage": "https://github.com/VSEphpbb/passwordstrength",
	"version": "1.1.0",
	"keywords": ["phpbb", "extension", "password", "strength"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Matt Friedman (VSE)",
			"homepage": "http://mattfriedman.me",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Password Strength",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0-RC2,<3.2.*@dev"
		},
		"version-check": {
			"host": "vsephpbb.github.io",
			"directory": "/version",
			"filename": "passwordstrength.json"
		}
	}
}
